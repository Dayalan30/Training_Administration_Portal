package tap.app.entities;

public class TrainerFeedBack {

private FeedBack feedback;
private Trainer trainer;
private double avg;
public TrainerFeedBack() {
	super();
	
}
public TrainerFeedBack(FeedBack feedback, Trainer trainer) {
	super();
	this.feedback = feedback;
	this.trainer = trainer;
}

public TrainerFeedBack(FeedBack feedback, Trainer trainer, double avg) {
	super();
	this.feedback = feedback;
	this.trainer = trainer;
	this.avg = avg;
}
public FeedBack getFeedback() {
	return feedback;
}
public void setFeedback(FeedBack feedback) {
	this.feedback = feedback;
}
public Trainer getTrainer() {
	return trainer;
}
public void setTrainer(Trainer trainer) {
	this.trainer = trainer;
}

public double getAvg() {
	return avg;
}
public void setAvg(double avg) {
	this.avg = avg;
}
@Override
public String toString() {
	return "TrainerFeedBack [feedback=" + feedback + ", trainer=" + trainer + ", avg=" + avg + "]";
}

}
